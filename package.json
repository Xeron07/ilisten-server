{
  "name": "inventory-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn client:build && node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "yarn --cwd admin start",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "client:build": "yarn --cwd admin install && yarn --cwd admin build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false yarn --cwd admin install && yarn --cwd admin build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.35.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "jnaudiostream": "^1.0.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.4",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "react-feather": "^2.0.10",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "throttle": "^1.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.22"
  }
}
